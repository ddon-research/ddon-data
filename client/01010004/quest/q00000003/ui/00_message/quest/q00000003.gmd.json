    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "帰還、遠距離移動には《リム転移》が便利です",
    "MessageIndex" : 3
