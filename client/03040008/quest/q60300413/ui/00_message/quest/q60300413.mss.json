      "MsgSerial" : 650595,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 5,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "But ever since someone\nstole them from the temple,\nthey've never been seen again.",
        "Jp" : "しかし、ある時\n何者かによって神殿から盗まれて以来\n一度も見つかっていないのだ"
      },
      "MsgSerial" : 650596,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
