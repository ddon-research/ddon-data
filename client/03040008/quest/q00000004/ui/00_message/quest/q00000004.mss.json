    "GroupSerial" : 7537,
    "GroupType" : 0,
    "GroupTypeName" : "MSG_GROUP_TYPE_NONE",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 15,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "Oh, you--\nI'm sorry to hear about your \ntroubles the other day.",
        "Jp" : "おお、貴君は――\nいつぞやはご苦労だったな"
      },
      "MsgSerial" : 20053,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 16,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "Let me greet you again.\nI am the Area Master of the Hidell Plains,\nAlfred the Arisen.",
        "Jp" : "あらためて挨拶をさせてもらおう\nわたしはハイデル平原のエリアマスター\n覚者のアルフレドだ"
      },
      "MsgSerial" : 20054,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
