  "NativeMsgGroupArray" : [ {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23744,
    "GroupType" : 1,
    "GroupTypeName" : "MSG_GROUP_TYPE_NORMAL",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 0,
      "JumpGroupSerial" : 0,
      "Message" : "Whatever it may be, Ser Arisen,\nIf there is something I can do,\nI would be happy to assist you.",
      "MsgSerial" : 586688,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 7
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 30755,
    "GroupType" : 121,
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 1,
      "JumpGroupSerial" : 0,
      "Message" : "Firefall Mountain is the place\nwhere the Fire Dragon takes its rest.\nOnly members of the Royal Family\nare allowed to enter.",
      "MsgSerial" : 785593,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 4
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 30756,
    "GroupType" : 121,
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 2,
      "JumpGroupSerial" : 0,
      "Message" : "It begins here.\nTo help Acre Selund soar once more,\nPlease lend us your strength.",
      "MsgSerial" : 785594,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 6
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 30757,
    "GroupType" : 121,
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 3,
      "JumpGroupSerial" : 0,
      "Message" : "Returning to this place—\nThough my feelings are complex,\nabove all, I am grateful for the Arisen.",
      "MsgSerial" : 785595,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 5
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23745,
    "GroupType" : 0,
    "GroupTypeName" : "MSG_GROUP_TYPE_NONE",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 4,
      "JumpGroupSerial" : 0,
      "Message" : "Have you already had the opportunity\nto meet Queen Cress' High Scepter, Kirsty?",
      "MsgSerial" : 652011,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 5,
      "JumpGroupSerial" : 0,
      "Message" : "Save for Lady Cress,\nhe is whimsical and difficult at times,\nbut... his skills are undoubtedly impressive.",
      "MsgSerial" : 787541,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23746,
    "GroupType" : 0,
    "GroupTypeName" : "MSG_GROUP_TYPE_NONE",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 6,
      "JumpGroupSerial" : 0,
      "Message" : "For monsters to infest Firefall Mountain,\nwhere the Flame Dragon rests--\nIt's unprecedented.",
      "MsgSerial" : 652012,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 7,
      "JumpGroupSerial" : 0,
      "Message" : "- Perhaps I, too, had such misconceptions\nand moments of carelessness.",
      "MsgSerial" : 776062,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23747,
    "GroupType" : 0,
    "GroupTypeName" : "MSG_GROUP_TYPE_NONE",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 8,
      "JumpGroupSerial" : 0,
      "Message" : "The blacksmithing skills\nof the Dwarf Orcs are quite impressive.\nTo the eyes of humans,\ntheir armors all appear rough,\nbut they are said to possess performance\nthat can only be created by them.",
      "MsgSerial" : 652013,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23770,
    "GroupType" : 42,
    "GroupTypeName" : "MSG_GROUP_TYPE_INTERVAL_TALK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 9,
      "JumpGroupSerial" : 0,
      "Message" : "Firefall Mountain is the place\nwhere the Lord Fire Dragon takes his rest.\nNow, it has become a den of demons.\nThe gate is tightly sealed,\nand only I am able to open it.",
      "MsgSerial" : 652010,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 8
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 10,
      "JumpGroupSerial" : 0,
      "Message" : "In other words, there is no need to worry\nabout people entering without cause.",
      "MsgSerial" : 722863,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 9
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 11,
      "JumpGroupSerial" : 0,
      "Message" : "It is most regrettable, but...\nNow that things have turned out like this,\nAll I could do was run away.",
      "MsgSerial" : 722864,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 10
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 12,
      "JumpGroupSerial" : 0,
      "Message" : "At the very least, as an Area Master,\nI hope I can support you, Ser Arisen.",
      "MsgSerial" : 722865,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 11
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23771,
    "GroupType" : 17,
    "GroupTypeName" : "MSG_GROUP_TYPE_PAWN_UNION",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 13,
      "JumpGroupSerial" : 0,
      "Message" : "Ser Arisen, please take this...\nIt may not be the most reliable of supplies,\nbut it might be of some use to you.",
      "MsgSerial" : 652024,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23772,
    "GroupType" : 18,
    "GroupTypeName" : "MSG_GROUP_TYPE_INN",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 14,
      "JumpGroupSerial" : 0,
      "Message" : "I will hand over this week's supplies to you,\nSer Arisen.\nI hope that these items can bring\na little brightness to your journey.",
      "MsgSerial" : 652025,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23773,
    "GroupType" : 19,
    "GroupTypeName" : "MSG_GROUP_TYPE_INN_FREE",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 15,
      "JumpGroupSerial" : 0,
      "Message" : "Ah, I was just in the middle\nof preparing the supplies.\n- I am glad that I am able to arrange\nsuch a large quantity of items\nIt brings me joy as well.",
      "MsgSerial" : 652026,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23774,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 16,
      "JumpGroupSerial" : 0,
      "Message" : "The soldiers of the Liberation Army\nsaid there may be rare materials around here.\nSer Arisen, how about seeking them out\nand strengthening your equipment?",
      "MsgSerial" : 652027,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 12
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 17,
      "JumpGroupSerial" : 0,
      "Message" : "I will also show you some of the best caves\nthat I know of for collecting!",
      "MsgSerial" : 722866,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 13
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23775,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 18,
      "JumpGroupSerial" : 0,
      "Message" : "We have received a report that a group\nof Ancestor Orcs has gathered at the mountaintop.\nThey are extremely primitive Orcs\nthat reside in the mountains.",
      "MsgSerial" : 652028,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 14
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 19,
      "JumpGroupSerial" : 0,
      "Message" : "While their intelligence may be inferior\nto that of Dwarf Orcs, in terms of strength,\nthey are easily the more dangerous creature.\nThey don't usually form such large groups, but...\nCould this also be due to the influence\nof the Evil Dragon?",
      "MsgSerial" : 722867,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 15
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 20,
      "JumpGroupSerial" : 0,
      "Message" : "If left alone, they will eventually come down\nfrom the mountains to attack this area as well.\nWe must scatter the herd as soon as possible!",
      "MsgSerial" : 722868,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 16
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23776,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 21,
      "JumpGroupSerial" : 0,
      "Message" : "The other day, thanks to the removal of the Orcs,\nthe merchants who were scared off\nhave started coming to this place as well.\nIt seems that we will be able to provide\nbetter support goods than ever before.",
      "MsgSerial" : 652029,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 17
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23777,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 22,
      "JumpGroupSerial" : 0,
      "Message" : "What kind of face do I present at the gate now?\nBack then, I barely survived. \nBut now, there are the Arisen,\nthe Liberation Army, and even the bandits.\n-- It's reassuring.",
      "MsgSerial" : 652030,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 18
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 23,
      "JumpGroupSerial" : 0,
      "Message" : "Master Arisen,\nbeyond the gate lies a den of demons.\nThey roam freely, seeking to extinguish\nthe flames of life that burn within those\nwho dare to tread this path.",
      "MsgSerial" : 722871,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 19
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 24,
      "JumpGroupSerial" : 0,
      "Message" : "The power to move forward\nlies only with the esteemed Arisen.\nBeyond the gate,\nthere is nary a village nor stronghold.\nPlease proceed with caution.",
      "MsgSerial" : 722872,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 20
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23778,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 25,
      "JumpGroupSerial" : 0,
      "Message" : "Ser Arisen, have you strengthened your equipment?\nIt seems that the warriors of the Liberation Army\nare obtaining new materials one after another.\nIt may be good to share information with them.",
      "MsgSerial" : 652043,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 22
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 26,
      "JumpGroupSerial" : 0,
      "Message" : "In various places, there are doors sealed\nby the command of the Royal Family.\nLet us release them under my name.\nFor it is in the path opened by the Arisen\nthat the future of this country shall flourish.",
      "MsgSerial" : 722874,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 21
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 27,
      "JumpGroupSerial" : 0,
      "Message" : "Allow me to also inform you\nof the whereabouts of valuable resources.\nThey are highly dangerous, and until now,\nI hesitated to share this information.\nBut... in the presence of a wise Arisen,\nit is a place that holds no fears.",
      "MsgSerial" : 785592,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 23
    } ],
    "NameDispOff" : false,
    "NpcId" : 4512,
    "NpcName" : "Bacias"
  }, {
    "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgGroup",
    "GroupNameSerial" : 0,
    "GroupSerial" : 23779,
    "GroupType" : 41,
    "GroupTypeName" : "MSG_GROUP_TYPE_CRAFT_BLOCK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 28,
      "JumpGroupSerial" : 0,
      "Message" : "Acre Selund, the Land of Flames.\nPerhaps due to this, even in death,\nthere are beings that wander,\nclad in flames, like \"shadows.\"\nThey will be seen roaming about.",
      "MsgSerial" : 652044,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 24
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 29,
      "JumpGroupSerial" : 0,
      "Message" : "You may have already seen it, Arisen.\nFor example, those who fly in the sky,\nand those who race on the ground—\n\"Flames\" that take on the form of such beings.",
      "MsgSerial" : 722875,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 25
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 30,
      "JumpGroupSerial" : 0,
      "Message" : "In places where Dragon Force accumulates,\nthey are attracted to it and gather together.\nThat's also a possibility.",
      "MsgSerial" : 722876,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 26
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 31,
      "JumpGroupSerial" : 0,
      "Message" : "Please be careful.\nEven though it may be a shadow clad in flames,\ndo not underestimate it,\nas it could inflict serious injuries.",
