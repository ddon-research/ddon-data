      "DispType" : 0,
      "GmdIndex" : 29,
      "JumpGroupSerial" : 0,
      "Message" : "Lookout Castle, which governs the southern part\nof this continent--\nThis castle, with its royal lineage, was not only\nimportant in terms of military affairs, but also\nin trade and culture.",
      "MsgSerial" : 586737,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 16
