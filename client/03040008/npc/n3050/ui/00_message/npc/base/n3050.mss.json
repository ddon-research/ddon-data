      "GmdIndex" : 20,
      "JumpGroupSerial" : 0,
      "Message" : "Let us cooperate and do everything we can.\nFirst, I will introduce a nearby and safe location\nfor you.",
      "MsgSerial" : 606871,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
