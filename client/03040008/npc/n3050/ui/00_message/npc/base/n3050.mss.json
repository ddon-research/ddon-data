      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 12,
      "JumpGroupSerial" : 0,
      "Message" : "Hehe, now--\nYes, it's something to be honestly happy about!",
      "MsgSerial" : 642245,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
